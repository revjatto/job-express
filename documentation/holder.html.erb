<p id="notice"><%= notice %></p>

<h1>Listing Jobs</h1>


    <% @jobs.each do |job| %>

        <p><%= job.title %></p>
        <p><%= job.company %></p>
        <p><%= job.location %></p>
        <p><%= job.salary %></p>
        <p><%= job.posted_by %></p>
        <p><%= job.description %></p>
        <%= link_to 'Show', job %>
        <%= link_to 'Edit', edit_job_path(job) %>
        <%= link_to 'Destroy', job, method: :delete, data: { confirm: 'Are you sure?' } %>

    <% end %>



<br>

<%= link_to 'New Job', new_job_path %>


